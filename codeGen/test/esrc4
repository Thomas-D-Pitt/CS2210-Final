/* ex4: Arrays */
program ex4;
class c4
{
	declarations
		int[] x=int[10];
		int[] ia[]={3, 5, 7};
	enddeclarations
	method void main()
	declarations
		int[] y=int [25];
		int z=-1;
	enddeclarations
	{
	y[3] := 17; 
	while (z<2)
	{
	  ia[z+1] := 1 + z*2 + z*z;
	  z:=z+1;
	};
        z := 0;
	while (z<=2)
	{
	  system.println(ia[z]);
	  z:=z+1;
	};
	}
}
